> cc -shared -fPIC -L/usr/local/lib -lbsdconv -I/usr/local/include/mysql -I/usr/local/include -o udf_bsdconv.so udf_bsdconv.c

> sudo cp udf_bsdconv.so /usr/local/lib/mysql/plugin/

> mysql -u root
mysql> CREATE FUNCTION Bsdconv RETURNS STRING SONAME 'udf_bsdconv.so';
Query OK, 0 rows affected (0.00 sec)

mysql> select bsdconv('utf-8:full:upper:utf-8','test');
+------------------------------------------+
| bsdconv('utf-8:full:upper:utf-8','test') |
+------------------------------------------+
| ＴＥＳＴ                             |
+------------------------------------------+
1 row in set (0.01 sec)

mysql> DROP FUNCTION bsdconv;
